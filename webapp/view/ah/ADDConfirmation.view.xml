		<App id="app">
			<pages>-->
	<Page id="page" title="AH C 610">
		<content>
			<VBox class="confirmationOuterVBox">
				<VBox class="bgColorWhite confirmationInnerVBox sapUiSmallMargin">
					<VBox class="sapUiLargeMargin">
						<FlexBox direction="Column" alignItems="Center">
							<items>
								<Title text="Thank you" class="thankyouTitle"/>
								<Text text="Your request is submitted and is pending approval." class="sapUiSmallMarginTop"/>
								<VBox class="bgColorLightGrey jobVBox sapUiSmallMargin">
									<FlexBox direction="Column" alignItems="Center">
										<items>
											<Text text="Job No. 1234567" class="jobText"/>
										</items>
									</FlexBox>
								</VBox>
								<Button text="Back to Dashboard" type="Emphasized" class="sapUiSmallMarginTopBottom" press="onBackToDashboard"/>
							</items>
						</FlexBox>
					</VBox>
				</VBox>
			</VBox>
		</content>
	</Page>
