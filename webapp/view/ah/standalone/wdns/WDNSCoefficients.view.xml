<mvc:View controllerName="avmet.ah.controller.WDNSCoefficients" xmlns:mvc="sap.ui.core.mvc" xmlns:l="sap.ui.layout" displayBlock="true"
	xmlns="sap.m" xmlns:core="sap.ui.core">
	<Page title="WDNS Coefficients Records - Aircraft OFP" showNavButton="true" enableScrolling="true" navButtonPress="onNavBack">
		<content>
			<VBox class="sapUiSmallMargin">
				<HBox justifyContent="End">
					<Button text="Print" icon="sap-icon://print" press="onPrint"/>
					<Button text="View Past Record" type="Transparent" class="sapUiTinyMarginBeginEnd" press="onViewPastRecord"></Button>
					<Button text="Update Record" type="Emphasized" press="onUpdateRecord"></Button>
				</HBox>
				<HBox>
					<VBox width="auto">
						<Table id="tblHarmonisation" columns="{oWDNSModel>/horm}" class="TableHeaderBGColor">
							<columns>
								<Column hAlign="Center">
									<Label text="{oWDNSModel>coltxt}" wrapping="true" design="Bold"/>
								</Column>
							</columns>
						</Table>
					</VBox>
					
				</HBox>
			</VBox>
		</content>
	</Page>
</mvc:View>