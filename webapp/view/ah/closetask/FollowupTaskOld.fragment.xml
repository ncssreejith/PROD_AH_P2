<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core" xmlns:l="sap.ui.layout" xmlns:cc="avmet.ah.control">
	<VBox backgroundDesign="Transparent" class="sapUiSmallMargin" items="{TaskModel>/}">
		<items>
			<l:Splitter height="auto">
				<VBox height="8rem" backgroundDesign="Solid">
					<l:Grid hSpacing="1" class="sapUiSmallMarginTop" defaultSpan="XL12 L12 M12 S12">
						<l:content>
							<VBox >
								<Label design="Bold" text="Task"/>
								<layoutData>
									<l:GridData linebreak="false" span="XL3 L3 M3 S12"/>
								</layoutData>
							</VBox>
							<VBox>
								<Text text="{oViewModel>/Task}"/>
								<layoutData>
									<l:GridData linebreak="true" span="XL12 L12 M12 S12"/>
								</layoutData>
							</VBox>
						</l:content>
					</l:Grid>
					<layoutData >
						<l:SplitterLayoutData size="250px" resizable="false"/>
					</layoutData>
				</VBox>
				<VBox>
					<l:Splitter height="auto">
						<VBox height="12rem" backgroundDesign="Solid">
							<l:Grid hSpacing="1" class="sapUiSmallMarginTop" defaultSpan="XL3 L3 M3 S12">
								<l:content>
									<VBox >
										<Label design="Bold" text="Date"/>
										<DatePicker fieldGroupIds="fgDate" displayFormat="dd.MM.YYY" required="{oViewModel>/singelSelect}" width="10rem"
											value="{oViewModel>createdData}" valueStateText="{i18n>MSG_DATE}"/>
										<layoutData>
											<l:GridData linebreak="false" span="XL3 L3 M3 S12"/>
										</layoutData>
									</VBox>
									<VBox>
										<Label design="Bold" text="Time"/>
										<TimePicker width="10rem" fieldGroupIds="fgTime" required="{oViewModel>/singelSelect}" displayFormat="HH:mm"
											valueStateText="{i18n>MSG_TIME}" value="{oViewModel>createdTime}"/>
										<layoutData>
											<l:GridData linebreak="true" span="XL3 L3 M3 S12"/>
										</layoutData>
									</VBox>
								</l:content>
							</l:Grid>
							<layoutData >
								<l:SplitterLayoutData size="250px" resizable="true"/>
							</layoutData>
						</VBox>
						<VBox>
							<VBox backgroundDesign="Solid">
								<!--visible="common"-->
								<l:Grid hSpacing="1" class="sapUiSmallMarginTop" defaultSpan="XL8 L8 M12 S12">
									<VBox>
										<Label text="Follow-Up Task"></Label>
										<Input fieldGroupIds="fgInput" required="{oViewModel>/singelSelect}" value="{oViewModel>FollowUpTask}"></Input>
										<layoutData>
											<l:GridData span="XL12 L12 M12 S12" linebreak="true"/>
										</layoutData>
									</VBox>
								</l:Grid>
							</VBox>
							<VBox backgroundDesign="Solid">
								<l:Grid hSpacing="1" defaultSpan="XL8 L8 M12 S12">
									<VBox>
										<Label text="{i18n>lblTechRef}"></Label>
										<Input fieldGroupIds="fgInput" required="{oViewModel>/singelSelect}" value="{oViewModel>TechicalReference}"></Input>
										<layoutData>
											<l:GridData span="XL6 L6 M10 S12" linebreak="true"/>
										</layoutData>
									</VBox>
								</l:Grid>
							</VBox>
							
							<VBox backgroundDesign="Solid" visible="{= ${TaskModel>tt1id} !== 'TT1_14' ?true:false }">
								<!--visible="OTHERS"  DOne-->
								<l:Grid hSpacing="2" class="sapUiSmallMarginTop" defaultSpan="XL8 L8 M12 S12">
									<VBox>
										<Label text="Describe Task"></Label>
										<TextArea maxLength="100" value="{oCreateTaskModel>/sOpsDesc}" showExceededText="true" width="350px" rows="5" fieldGroupIds="fgTxtArea"
											required="{oCreateTaskModel>/bOpsCheck}"/>
										<layoutData>
											<l:GridData span="XL4 L4 M6 S12" linebreak="true"/>
										</layoutData>
									</VBox>
									<VBox>
										<Label text="{i18n>lblTechRef}"></Label>
										<Input fieldGroupIds="fgInput" required="{oViewModel>/singelSelect}" value="{oViewModel>TechicalReference}"></Input>
										<layoutData>
											<l:GridData span="XL4 L4 M6 S12" linebreak="true"/>
										</layoutData>
									</VBox>
								</l:Grid>
								<l:Grid hSpacing="2" class="sapUiSmallMarginTop" defaultSpan="XL8 L8 M12 S12">
									<HBox class="sapUiSmallMarginBeginEnd sapUiSmallMarginBottom">
										<VBox>
											<Label design="Bold" text="But not later than" width="350px"/>
											<SegmentedButton selectedKey="{oViewGlobalModel>/OPPR}" selectionChange="onReasonTypeChange" fieldGroupIds="fgSGBtn">
												<items>
													<SegmentedButtonItem text="Date" key="D" width="50%"/>
													<SegmentedButtonItem text="Utilisation" key="U" width="50%"/>
													<SegmentedButtonItem text="Both" key="B" width="50%"/>
													<SegmentedButtonItem text="" key="N" visible="false"/>
												</items>
											</SegmentedButton>
										</VBox>
									</HBox>
									<HBox class="sapUiSmallMarginBeginEnd sapUiSmallMarginBottom" visible="{oViewModel>/bDateSection}">
										<VBox>
											<Label design="Bold" text="Expiry Date" width="150px"/>
											<DatePicker
												value="{path: 'oViewGlobalModel>/EXPDT', type: 'sap.ui.model.type.Date', formatOptions: {strictParsing: true , UTC: true, pattern:'dd/MM/yyyy'}}"
												width="200px" fieldGroupIds="fgDate" required="{oViewModel>/bDateSection}"/>
										</VBox>
										<VBox class="sapUiSmallMarginBegin">
											<Label design="Bold" text="Time" width="150px"/>
											<TimePicker width="200px" value="{oViewGlobalModel>/EXPTM}" valueFormat="HH:mm" displayFormat="HH:mm" fieldGroupIds="fgTime"
												required="{oViewModel>/bDateSection}"/>
										</VBox>
									</HBox>
									<HBox class="sapUiSmallMarginBeginEnd sapUiSmallMarginBottom" visible="{oViewModel>/bUtilisationSection}">
										<VBox>
											<Label design="Bold" text="Utilisation" width="150px"/>
											<ComboBox width="200px" items="{UtilizationCBModel>/}" selectedKey="{oViewGlobalModel>/UTIL1}" change="onUilisationChange"
												fieldGroupIds="fgCmbBox" required="{oViewModel>/bUtilisationSection}">
												<core:Item text="{UtilizationCBModel>description}" key="{UtilizationCBModel>ddid}"/>
											</ComboBox>
										</VBox>
										<VBox class="sapUiSmallMarginBegin">
											<Label/>
											<StepInput width="200px" value="{oViewGlobalModel>/UTILVL}" visible="{oViewModel>/bAirFrameAndTAC}" required="{oViewModel>/bAirFrameAndTAC}"/>
											<ComboBox width="200px" items="{Utilization2CBModel>/}" visible="{oViewModel>/bScheduleService}" selectedKey="{oViewGlobalModel>/UTIL2}"
												fieldGroupIds="fgCmbBox" required="{oViewModel>/bScheduleService}">
												<core:Item text="{Utilization2CBModel>description}" key="{Utilization2CBModel>ddid}"/>
											</ComboBox>
											<Input value="52345" editable="false" visible="{oViewModel>/bPhaseService}" fieldGroupIds="fgInput" required="{oViewModel>/bPhaseService}"/>
										</VBox>
										<VBox class="sapUiSmallMarginBegin" visible="{oViewModel>/bScheduleService}">
											<Label/>
											<Input value="4550 AF Hrs" editable="false"/>
										</VBox>
									</HBox>
									<HBox class="sapUiSmallMarginBeginEnd sapUiSmallMarginBottom" visible="{oViewModel>/bLimitationSection}">
										<VBox>
											<HBox visible="{oViewModel>/bLimitation}">
												<Label design="Bold" text="Limitation"/>
												<core:Icon src="sap-icon://sys-help-2" size="12px" class="sapUiTinyMarginBegin" color="#031E48"/>
											</HBox>
											<TextArea width="400px" value="{oViewGlobalModel>/LDESC}" visible="{oViewModel>/bLimitation}" maxLength="100" showExceededText="true"
												fieldGroupIds="fgTxtArea" required="{oViewModel>/bLimitation}"/>
											<Button icon="sap-icon://sys-add" text="Add Limitation" width="400px" class="sapUiTinyMarginTop addLimitationBtn addTemBtn"
												press="onAddLimitaionPress" visible="{oViewModel>/bAddLimitationBtn}" iconFirst="true"/>
											<Button icon="sap-icon://sys-minus" text="Remove Limitation" width="400px" class="sapUiTinyMarginTop removeLimitationBtn"
												press="onRemoveLimitaionPress" visible="{oViewModel>/bLimitation}"/>
										</VBox>
									</HBox>
								</l:Grid>
							</VBox>
							<VBox backgroundDesign="Solid" visible="{= ${TaskModel>tt1id} !== 'TT1_11' ?true:false }">
								<!--visible="OPS Check" DOne-->
								<l:Grid hSpacing="2" class="sapUiSmallMarginTop" defaultSpan="XL8 L8 M12 S12">
									<VBox>
										<Label text="Describe Task"></Label>
										<TextArea maxLength="100" value="{oCreateTaskModel>/sOpsDesc}" showExceededText="true" width="350px" rows="5" fieldGroupIds="fgTxtArea"
											required="{oCreateTaskModel>/bOpsCheck}"/>
										<layoutData>
											<l:GridData span="XL4 L4 M6 S12" linebreak="true"/>
										</layoutData>
									</VBox>
									<VBox>
										<Label text="Findings"></Label>
										<TextArea maxLength="100" value="{oCreateTaskModel>/sOpsDesc}" showExceededText="true" width="350px" rows="5" fieldGroupIds="fgTxtArea"
											required="{oCreateTaskModel>/bOpsCheck}"/>
										<layoutData>
											<l:GridData span="XL4 L4 M6 S12" linebreak="true"/>
										</layoutData>
									</VBox>
									<VBox>
										<Label text="{i18n>lblTechRef}"></Label>
										<Input fieldGroupIds="fgInput" required="{oViewModel>/singelSelect}" value="{oViewModel>TechicalReference}"></Input>
										<layoutData>
											<l:GridData span="XL4 L4 M6 S12" linebreak="true"/>
										</layoutData>
									</VBox>
								</l:Grid>
							</VBox>
							<!--//Remove and Insatll- Replacement- done-->
							<VBox backgroundDesign="Solid" visible="{= ${TaskModel>tt1id}==='TT1_10' &amp;&amp; ${TaskModel>tt2id}==='TT2_10' ?true:false}">
								<l:Grid hSpacing="2" class="sapUiSmallMarginTop" defaultSpan="XL8 L8 M12 S12">
									<VBox>
										<HBox>
											<VBox>
												<HBox>
													<Label design="Bold" text="Type"/>
													<core:Icon src="sap-icon://sys-help-2" class="sapUiTinyMarginBegin" size="12px" color="#031E48"/>
												</HBox>
												<ComboBox id="Type1Id" width="170px" change="onTypeChange" selectedKey="{oCreateTaskModel>/sType1}" fieldGroupIds="fgCmbBox"
													required="{oCreateTaskModel>/bType}" items="{TT1Model>/}">
													<core:Item text="{TT1Model>key}" key="{TT1Model>text}"/>
												</ComboBox>
											</VBox>
											<VBox>
												<HBox>
													<Label design="Bold" text="Type"/>
													<core:Icon src="sap-icon://sys-help-2" class="sapUiTinyMarginBegin" size="12px" color="#031E48"/>
												</HBox>
												<ComboBox width="170px" change="onTypeChange" selectedKey="{oCreateTaskModel>/sType2}" fieldGroupIds="fgCmbBox"
													required="{oCreateTaskModel>/bType}" items="{TT2Model>/}">
													<core:Item text="{TT2Model>key}" key="{TT2Model>text}"/>
												</ComboBox>
											</VBox>
										</HBox>
										<layoutData>
											<l:GridData span="XL4 L4 M6 S12" linebreak="true"/>
										</layoutData>
									</VBox>
									<VBox>
										<HBox>
											<VBox>
												<HBox>
													<Label/>
													<Text text="Optional" class="optionalTxtMargin" visible="{oCreateTaskModel>/bOptionalLabel}"/>
												</HBox>
												<Input width="170px" value="{oCreateTaskModel>/sType1Value}" fieldGroupIds="fgInput" required="{oCreateTaskModel>/bType}"/>
											</VBox>
											<VBox>
												<HBox>
													<Label/>
													<Text text="Optional" class="optionalTxtMargin" visible="{oCreateTaskModel>/bOptionalLabel}"/>
												</HBox>
												<Input width="170px" value="{oCreateTaskModel>/sType2Value}" fieldGroupIds="fgInput" required="{oCreateTaskModel>/bType}"/>
											</VBox>
										</HBox>
										<layoutData>
											<l:GridData span="XL4 L4 M6 S12" linebreak="true"/>
										</layoutData>
									</VBox>
								</l:Grid>
								<l:Grid hSpacing="2" class="sapUiSmallMarginTop" defaultSpan="XL8 L8 M12 S12">
									<HBox class="sapUiSmallMarginBeginEnd sapUiSmallMarginBottom">
										<VBox>
											<Label design="Bold" text="But not later than" width="350px"/>
											<SegmentedButton selectedKey="{oViewGlobalModel>/OPPR}" selectionChange="onReasonTypeChange" fieldGroupIds="fgSGBtn">
												<items>
													<SegmentedButtonItem text="Date" key="D" width="50%"/>
													<SegmentedButtonItem text="Utilisation" key="U" width="50%"/>
													<SegmentedButtonItem text="Both" key="B" width="50%"/>
													<SegmentedButtonItem text="" key="N" visible="false"/>
												</items>
											</SegmentedButton>
										</VBox>
									</HBox>
									<HBox class="sapUiSmallMarginBeginEnd sapUiSmallMarginBottom" visible="{oViewModel>/bDateSection}">
										<VBox>
											<Label design="Bold" text="Expiry Date" width="150px"/>
											<DatePicker
												value="{path: 'oViewGlobalModel>/EXPDT', type: 'sap.ui.model.type.Date', formatOptions: {strictParsing: true , UTC: true, pattern:'dd/MM/yyyy'}}"
												width="200px" fieldGroupIds="fgDate" required="{oViewModel>/bDateSection}"/>
										</VBox>
										<VBox class="sapUiSmallMarginBegin">
											<Label design="Bold" text="Time" width="150px"/>
											<TimePicker width="200px" value="{oViewGlobalModel>/EXPTM}" valueFormat="HH:mm" displayFormat="HH:mm" fieldGroupIds="fgTime"
												required="{oViewModel>/bDateSection}"/>
										</VBox>
									</HBox>
									<HBox class="sapUiSmallMarginBeginEnd sapUiSmallMarginBottom" visible="{oViewModel>/bUtilisationSection}">
										<VBox>
											<Label design="Bold" text="Utilisation" width="150px"/>
											<ComboBox width="200px" items="{UtilizationCBModel>/}" selectedKey="{oViewGlobalModel>/UTIL1}" change="onUilisationChange"
												fieldGroupIds="fgCmbBox" required="{oViewModel>/bUtilisationSection}">
												<core:Item text="{UtilizationCBModel>description}" key="{UtilizationCBModel>ddid}"/>
											</ComboBox>
										</VBox>
										<VBox class="sapUiSmallMarginBegin">
											<Label/>
											<StepInput width="200px" value="{oViewGlobalModel>/UTILVL}" visible="{oViewModel>/bAirFrameAndTAC}" required="{oViewModel>/bAirFrameAndTAC}"/>
											
										</VBox>
										<VBox class="sapUiSmallMarginBegin" visible="{oViewModel>/bScheduleService}">
											<Label/>
											<Input value="4550 AF Hrs" editable="false"/>
										</VBox>
									</HBox>
									<HBox class="sapUiSmallMarginBeginEnd sapUiSmallMarginBottom" visible="{oViewModel>/bLimitationSection}">
										<VBox>
											<HBox visible="{oViewModel>/bLimitation}">
												<Label design="Bold" text="Limitation"/>
												<core:Icon src="sap-icon://sys-help-2" size="12px" class="sapUiTinyMarginBegin" color="#031E48"/>
											</HBox>
											<TextArea width="400px" value="{oViewGlobalModel>/LDESC}" visible="{oViewModel>/bLimitation}" maxLength="100" showExceededText="true"
												fieldGroupIds="fgTxtArea" required="{oViewModel>/bLimitation}"/>
											<Button icon="sap-icon://sys-add" text="Add Limitation" width="400px" class="sapUiTinyMarginTop addLimitationBtn addTemBtn"
												press="onAddLimitaionPress" visible="{oViewModel>/bAddLimitationBtn}" iconFirst="true"/>
											<Button icon="sap-icon://sys-minus" text="Remove Limitation" width="400px" class="sapUiTinyMarginTop removeLimitationBtn"
												press="onRemoveLimitaionPress" visible="{oViewModel>/bLimitation}"/>
										</VBox>
									</HBox>
								</l:Grid>
							</VBox>
							<!--//Remove and Insatll-Troubleshoot/ Bay Check-->
							<VBox backgroundDesign="Solid" visible="{= ${TaskModel>tt1id}==='TT1_10' &amp;&amp; ${TaskModel>tt2id}==='TT2_10' ?true:false}">
								<l:Grid hSpacing="2" class="sapUiSmallMarginTop" defaultSpan="XL8 L8 M12 S12">
									<VBox>
										<HBox>
											<VBox>
												<HBox>
													<Label design="Bold" text="Type"/>
													<core:Icon src="sap-icon://sys-help-2" class="sapUiTinyMarginBegin" size="12px" color="#031E48"/>
												</HBox>
												<ComboBox width="170px" change="onTypeChange" selectedKey="{oCreateTaskModel>/sType1}" fieldGroupIds="fgCmbBox"
													required="{oCreateTaskModel>/bType}" items="{TT1Model>/}">
													<core:Item text="{TT1Model>key}" key="{TT1Model>text}"/>
												</ComboBox>
											</VBox>
											<VBox>
												<HBox>
													<Label design="Bold" text="Type"/>
													<core:Icon src="sap-icon://sys-help-2" class="sapUiTinyMarginBegin" size="12px" color="#031E48"/>
												</HBox>
												<ComboBox width="170px" change="onTypeChange" selectedKey="{oCreateTaskModel>/sType2}" fieldGroupIds="fgCmbBox"
													required="{oCreateTaskModel>/bType}" items="{TT2Model>/}">
													<core:Item text="{TT2Model>key}" key="{TT2Model>text}"/>
												</ComboBox>
											</VBox>
										</HBox>
										<layoutData>
											<l:GridData span="XL4 L4 M6 S12" linebreak="true"/>
										</layoutData>
									</VBox>
									<VBox>
										<HBox>
											<VBox>
												<HBox>
													<Label/>
													<Text text="Optional" class="optionalTxtMargin" visible="{oCreateTaskModel>/bOptionalLabel}"/>
												</HBox>
												<Input width="170px" value="{oCreateTaskModel>/sType1Value}" fieldGroupIds="fgInput" required="{oCreateTaskModel>/bType}"/>
											</VBox>
											<VBox>
												<HBox>
													<Label/>
													<Text text="Optional" class="optionalTxtMargin" visible="{oCreateTaskModel>/bOptionalLabel}"/>
												</HBox>
												<Input width="170px" value="{oCreateTaskModel>/sType2Value}" fieldGroupIds="fgInput" required="{oCreateTaskModel>/bType}"/>
											</VBox>
										</HBox>
										<layoutData>
											<l:GridData span="XL4 L4 M6 S12" linebreak="true"/>
										</layoutData>
									</VBox>
									<VBox>
										<Label text="Findings"></Label>
										<TextArea maxLength="100" value="{oCreateTaskModel>/sOpsDesc}" showExceededText="true" width="350px" rows="5" fieldGroupIds="fgTxtArea"
											required="{oCreateTaskModel>/bOpsCheck}"/>
										<layoutData>
											<l:GridData span="XL4 L4 M6 S12" linebreak="true"/>
										</layoutData>
									</VBox>
								</l:Grid>
							</VBox>
							<layoutData>
								<l:SplitterLayoutData size="auto"/>
							</layoutData>
						</VBox>
					</l:Splitter>
					<layoutData>
						<l:SplitterLayoutData size="auto"/>
					</layoutData>
				</VBox>
			</l:Splitter>
		</items>
	</VBox>
	<!--<VBox backgroundDesign="Transparent" visible="{=${ViewModel>/MulitiFlag}==='Y' ? true:false}" class="sapUiLargeMargin">
		<l:Splitter height="auto">
			<VBox backgroundDesign="Solid">
				<l:Grid hSpacing="1" class="sapUiSmallMarginTop" defaultSpan="XL12 L12 M12 S12">
					<l:content>
						<VBox >
							<Label design="Bold" text="Task"/>
							<layoutData>
								<l:GridData linebreak="false" span="XL3 L3 M3 S12"/>
							</layoutData>
						</VBox>
						<VBox>
							<Text text="Tools, publication and all consumables to be accounted for. Loose articles checks to be carried out."/>
							<layoutData>
								<l:GridData linebreak="true" span="XL12 L12 M12 S12"/>
							</layoutData>
						</VBox>
					</l:content>
				</l:Grid>
				<layoutData >
					<l:SplitterLayoutData size="250px" resizable="false"/>
				</layoutData>
			</VBox>
			<VBox>
				<l:Splitter height="auto">
					<VBox backgroundDesign="Solid">
						<l:Grid hSpacing="1" class="sapUiSmallMarginTop" defaultSpan="XL12 L12 M12 S12">
							<l:content>
								<VBox >
									<Label design="Bold" text="Start Date"/>
									<DatePicker editable="false" displayFormat="dd.MM.YYYY" width="10rem" dateValue="{TaskModel>/0/credtm}"/>
									<layoutData>
										<l:GridData linebreak="false" span="XL12 L12 M12 S12"/>
									</layoutData>
								</VBox>
								<VBox>
									<Label design="Bold" text="Start Time"/>
									<TimePicker width="10rem" editable="false" displayFormat="HH:mm" dateValue="{TaskModel>/0/creuzt}"/>
									<layoutData>
										<l:GridData linebreak="true" span="XL12 L12 M12 S12"/>
									</layoutData>
								</VBox>
								<VBox >
									<Label design="Bold" text="End Date"/>
									<DatePicker displayFormat="dd.MM.YYY" editable="false" width="10rem" dateValue="{TaskModel>/0/ftcredt}"/>
									<layoutData>
										<l:GridData linebreak="true" span="XL12 L12 M12 S12"/>
									</layoutData>
								</VBox>
								<VBox>
									<Label design="Bold" text="End Time"/>
									<TimePicker width="10rem" editable="false" displayFormat="HH:mm" dateValue="{TaskModel>/0/ftcretm}"/>
									<layoutData>
										<l:GridData linebreak="true" span="XL12 L12 M12 S12"/>
									</layoutData>
								</VBox>
							</l:content>
						</l:Grid>
						<layoutData >
							<l:SplitterLayoutData size="250px" resizable="true"/>
						</layoutData>
					</VBox>
					<VBox>
						<VBox backgroundDesign="Solid">
							<l:Grid hSpacing="2" class="sapUiSmallMarginTop" defaultSpan="XL12 L12 M12 S12">
								<HBox>
									<VBox >
										<Label text="Total Qty"></Label>
										<StepInput fieldGroupIds="fgStepInput" required="{oViewModel>/multiSelect}" value="{oViewModel>/totalQty}"></StepInput>
									</VBox>
									<VBox class="sapUiTinyMarginBegin">
										<Label text="Publication"></Label>
										<StepInput fieldGroupIds="fgStepInput" required="{oViewModel>/multiSelect}" value="{oViewModel>/Publication}"></StepInput>
									</VBox>
									<layoutData>
										<l:GridData span="XL12 L12 M12 S12" linebreak="true"/>
									</layoutData>
								</HBox>
								<Table mode="MultiSelect" class="sparesTable" selectionChange="onTaskSelectionChange" items="{TaskModel>/}"
									rememberSelections="true">
									<headerToolbar>
										<OverflowToolbar>
											<content>
												<Label text="{i18n>lblmultiTask}"/>
												<ToolbarSpacer/>
												<SearchField width="10rem"/>
											</content>
										</OverflowToolbar>
									</headerToolbar>
									<columns >
										<Column class="sparesTableTextColor">
											<Label class="whiteText" text="Task"/>
