<mvc:View controllerName="avmet.ah.controller.TransferToADD" xmlns:mvc="sap.ui.core.mvc" displayBlock="true" xmlns="sap.m"
	xmlns:core="sap.ui.core" xmlns:l="sap.ui.layout">
	<Page id="page" showHeader="false">
		<Toolbar id="tbId" height="4rem" class="backgroundWhite">
			<Button icon="sap-icon://arrow-left" type="Transparent" press="onNavBack" class="iconColorWhite sapUiMediumMarginBegin"></Button>
			<VBox>
				<Title text="Transfer Job to ADD" class="titleTextBoldToolbar sapUiTinyMarginTop"></Title>
				<Text text="{oViewModel>/sJobId}"/>
			</VBox>
		</Toolbar>
		<content>
			<!--<core:Fragment fragmentName="avmet.ah.view.ah.transfertoadd.TransferToADDDetails" type="XML"/>-->
		
		</content>
		<footer>
			<OverflowToolbar>
				<ToolbarSpacer/>
				<Button text="Submit" type="Emphasized" press="onSubmitTransferJobAdd"></Button>
			</OverflowToolbar>
		</footer>
	</Page>
</mvc:View>