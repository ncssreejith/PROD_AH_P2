								<!--Teck Meng change on 23/11/2020 10:00 start-->
								<Text text="{path:'oEngineModel>oEngineModel>TGTTAB', formatter:'.formatter.integerUnit'}"/>
								<!--Teck Meng change on 23/11/2020 10:00 end-->
								<Text text="{oEngineModel>ASPEED}"/>
								<HBox justifyContent="Center">
									<Text text="{path:'oEngineModel>ETF'}" visible="{= !${oEngineModel>/headerDetails/ETFEDIT} }" class="sapUiTinyMarginTopEnd"/>
									<StepInput min="{oEngineModel>minValue}" value="{oEngineModel>ETF}"
										visible="{= !!${oEngineModel>/headerDetails/ETFEDIT} &amp;&amp; !!${oEngineModel>Special} }" displayValuePrecision="2"/>
								</HBox>
								<Button text="{= !!${oEngineModel>/headerDetails/ETFEDIT} ? ${i18n>txtSignOff} : ${i18n>btnEdit} }" visible="{= !!${oEngineModel>Special} }"
									enabled="{= ${avmetModel>/dash/astid} !== 'AST_FFF' &amp;&amp; ${avmetModel>/dash/astid} !== 'AST_FFF0' ? true : false}"
									icon="sap-icon://request" press="onETFEdit" type="Emphasized"/>
