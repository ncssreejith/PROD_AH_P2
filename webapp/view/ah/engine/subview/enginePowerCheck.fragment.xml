					</columns>
					<items>
						<ColumnListItem>
							<cells>
								<Text text="{path:'oEngineModel>BEGDA', formatter:'.formatter.defaultDateFormatDisplay'}"/>
								<!--<Text text="{oEngineModel>TIMEI} {oEngineModel>TIMEI}"/>-->
								<!--Teck Meng change on 23/11/2020 10:00 start-->
								<Text text="{path:'oEngineModel>oEngineModel>TEMP', formatter:'.formatter.integerUnit'}"/>
								<!--Teck Meng change on 23/11/2020 10:00 end-->
								<!--Teck Meng change on 23/11/2020 10:00 start-->
								<Text text="{path:'oEngineModel>oEngineModel>BPRESS', formatter:'.formatter.integerUnit'}"/>
