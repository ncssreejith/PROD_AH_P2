<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core">
	<Dialog title="Create New Flight Servicing">
		<content>
			<VBox class="sapUiSmallMargin">
				<Text text="Do you want to create a new Flight Servicing" wrapping="true" class="sapUiTinyMargin"/>
			</VBox>
		</content>
		<beginButton>
			<Button text="Cancel" type="Transparent" press="onNewFSSignOffConfirm($event, 'C')"/>
		</beginButton>
		<endButton>
			<Button text="Yes" type="Emphasized" press="onNewFSSignOffConfirm($event, 'X')"/>
		</endButton>
	</Dialog>
</core:FragmentDefinition>