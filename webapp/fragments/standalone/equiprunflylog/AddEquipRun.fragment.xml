<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core">
	<Dialog title="">
		<content>
			<RadioButtonGroup buttons="{oAircraftUtilModel>/reasons}" selectedIndex="{oAircraftUtilModel>/selIndex}">
				<buttons>
					<RadioButton  text="{oAircraftUtilModel>text}"/>
				</buttons>
			</RadioButtonGroup>
		</content>
		<beginButton>
			<Button text="Cancel" type="Transparent" press="onCancelAddEquipCancel"/>
		</beginButton>
		<endButton>
			<Button text="Proceed" type="Emphasized" press="onProceedEquipRunLog"/>
		</endButton>
	</Dialog>
</core:FragmentDefinition>