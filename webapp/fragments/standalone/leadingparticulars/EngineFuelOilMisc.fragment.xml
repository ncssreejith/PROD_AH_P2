<core:FragmentDefinition xmlns:l="sap.ui.layout" xmlns="sap.m" xmlns:core="sap.ui.core">
	<VBox class="LeadPartFlexboxBGWhite" width="100%" alignItems="Center">
		<Table width="95%" showNoData="false" class="TableHeaderBGColor sapUiSmallMarginTop sapUiMediumMarginBeginEnd sapUiSizeCompact">
			<columns>
				<Column class="TableTextColor" hAlign="Left">
					<Label text="Component/System" design="Bold"/>
				</Column>
				<Column minScreenWidth="Tablet" demandPopin="true" hAlign="Left">
					<Label text="MIL or DTD Specification" design="Bold" wrapping="true"/>
				</Column>
				<Column minScreenWidth="Tablet" demandPopin="true" hAlign="Left">
					<Label text="Capacity" design="Bold" wrapping="true"/>
				</Column>
				<Column minScreenWidth="Tablet" demandPopin="true" hAlign="Left">
					<Label text="Type" design="Bold" wrapping="true"/>
				</Column>
			</columns>
		</Table>
		<Table width="95%" items="{ path: 'OverViewItemOILModel>/', filters : [{ path : 'OILTYP', operator : 'EQ', value1 : 'ENGOIL' }]}"
			class="TableHeaderBGColor sapUiMediumMarginBeginEnd sapUiSizeCompact">
			<columns>
				<Column class="TableTextColor" hAlign="Left">
					<Label text="Engine Oil" design="Bold"/>
				</Column>
				<Column minScreenWidth="Tablet" demandPopin="true" hAlign="Left"></Column>
				<Column minScreenWidth="Tablet" demandPopin="true" hAlign="Left"></Column>
				<Column minScreenWidth="Tablet" demandPopin="true" hAlign="Left"></Column>
			</columns>
			<items>
				<ColumnListItem>
					<cells>
						<VBox>
							<Text text="{OverViewItemOILModel>COMP}" visible="{= ${OverViewItemOILModel>/isTableEditable} ? false : true}"/>
							<Input value="{OverViewItemOILModel>COMP}" visible="{= ${OverViewItemOILModel>/isTableEditable} ? true : false}" change="onChangeOilValue"/>
						</VBox>
						<VBox>
							<Text text="{OverViewItemOILModel>MILDTDSPEC}" visible="{= ${OverViewItemOILModel>/isTableEditable} ? false : true}"/>
							<Input value="{OverViewItemOILModel>MILDTDSPEC}" visible="{= ${OverViewItemOILModel>/isTableEditable} ? true : false}"
								change="onChangeOilValue"/>
						</VBox>
						<VBox>
							<Text text="{OverViewItemOILModel>OILCAPASITY}" visible="{= ${OverViewItemOILModel>/isTableEditable} ? false : true}"/>
							<Input value="{OverViewItemOILModel>OILCAPASITY}" visible="{= ${OverViewItemOILModel>/isTableEditable} ? true : false}"
								change="onChangeOilValue"/>
						</VBox>
						<VBox>
							<Text text="{OverViewItemOILModel>FTYPE}" visible="{= ${OverViewItemOILModel>/isTableEditable} ? false : true}"/>
							<Input value="{OverViewItemOILModel>FTYPE}" visible="{= ${OverViewItemOILModel>/isTableEditable} ? true : false}" change="onChangeOilValue"/>
						</VBox>
					</cells>
				</ColumnListItem>
			</items>
		</Table>
