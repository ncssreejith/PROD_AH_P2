<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core">
	<Dialog showHeader="false">
		<content>
			<!--Teck Meng change on 25/11/2020 15:00 start-->
			<RadioButtonGroup class="sapUiSmallMargin" buttons="{path:'avmetModel>/runningChange'}"
			select="onRunningChangePressIndex">
				<!--Teck Meng change on 25/11/2020 15:00 end-->
				<buttons>
					<!--Teck Meng change on 25/11/2020 15:00 start-->
					<RadioButton text="Pilot name: {avmetModel>PILOTNAME} Updated on: {path: 'avmetModel>patime',formatter:'.formatter.defaultDateTimeFormat'}" />
					<!--Teck Meng change on 25/11/2020 15:00 end-->
				</buttons>
			</RadioButtonGroup>
		</content>
		<beginButton>
			<Button text="Cancel" type="Transparent" press="onRunningChangeCancel"/>
		</beginButton>
		<endButton>
			<Button text="Pilot Update" type="Emphasized" press="onRunningChangePress" />
		</endButton>
	</Dialog>
</core:FragmentDefinition>